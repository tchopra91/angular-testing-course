        "@angular/core": "10.0.2",
        "@angular/forms": "10.0.2",
        "@angular/material": "^10.0.1",
        "@angular/material-moment-adapter": "10.0.1",
        "@angular/platform-browser": "10.0.2",
        "@angular/platform-browser-dynamic": "10.0.2",
        "@angular/router": "10.0.2",
        "body-parser": "^1.18.3",
        "core-js": "^2.4.1",
        "cypress": "^3.8.3",
        "express": "^4.16.2",
        "http-server": "^0.12.1",
        "moment": "^2.22.2",
        "npm-run-all": "^4.1.5",
        "rxjs": "6.5.4",
